{
    "Common":{
        "description": "common config for all optimizers in this configuration",
        "budget": 50000,
        "budget_dim_ratio": 0,
        "f_module": "cars.code.problems.simple_funcs",
        "f_name": "rosenbrock",
        "record_x_history": true,
        "f_target": 1e-8,
        "verbose": 2
    },
    "CARS_simple":{
        "Otype": "CARS",
        "description": "config for CARS optimizer",
        "randgen": "uniform_sphere",
        "h": 0.01,
        "Lhat": 1.5
    },
    "CARS_CR":{
        "Otype": "CARS-CR",
        "description": "config for CARS-CR optimizer",
        "randgen": "uniform_sphere",
        "h": 0.01,
        "M": 1
    },
    "CARS_NQ":{
        "Otype": "CARS-NQ",
        "description": "config for CARS-NQ optimizer",
        "randgen": "uniform_sphere",
        "h": 0.01,
        "nq": 5
    },
    "Nesterov-Spokoiny":{
        "Otype": "Nesterov",
        "description": "config for Nesterov-Spokoiny optimizer. Rosenbrock function requires large L",
        "randgen": "gaussian",
        "h": 0.01,
        "L": 200000
    },
    
    "CARS_simple_large_h":{
        "Otype": "CARS",
        "description": "config for CARS optimizer with larger h",
        "randgen": "uniform_sphere",
        "h": 0.2,
        "Lhat": 1.5
    },
    "CARS_CR_large_h":{
        "Otype": "CARS-CR",
        "description": "config for CARS-CR optimizer with larger h",
        "randgen": "uniform_sphere",
        "h": 0.2,
        "M": 1
    },
    "CARS_NQ_large_h":{
        "Otype": "CARS-NQ",
        "description": "config for CARS-NQ optimizer with larger h",
        "randgen": "uniform_sphere",
        "h": 0.2,
        "nq": 5
    },
    "Nesterov-Spokoiny_large_h":{
        "Otype": "Nesterov",
        "description": "config for Nesterov-Spokoiny optimizer with larger h. Rosenbrock function requires large L",
        "randgen": "gaussian",
        "h": 0.2,
        "L": 200000
    }
}